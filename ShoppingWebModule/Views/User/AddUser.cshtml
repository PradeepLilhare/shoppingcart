@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
    <!-- Bootstrap Core Css -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/plugins/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/plugins/node-waves/waves.css" rel="stylesheet" />
    <link href="~/Content/plugins/animate-css/animate.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <script src="~/Content/plugins/morrisjs/morris.js"></script>
    <script src="~/Content/js/admin.js"></script>


    <link href="~/Content/css/themes/all-themes.css" rel="stylesheet" />
    <title>Add Product</title>
</head>
<body class="theme-red">
    <div>
        @*<div class="row clearfix">*@
        @*<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">*@
        <div class="card" style="margin-top:30px;margin-left:30px;margin-right:30px;margin-bottom:30px;">
            <div class="header">
                <h2>
                    Add User
                </h2>
            </div>
            <div style="text-align:left;margin-left:20px;margin-top:15px">
                <a href="@Url.Action("Index", "User")" class="elements">
                    <i class="fa fa-arrow-circle-left" style="font-size:48px;color:#1f91f3"></i>
                </a>

            </div>
            <div style="margin-left:40px;margin-right:50px;margin-top:10px;margin-bottom:20px; border:ridge;" class="body">
                <form>
                    @*<section class="content">*@
                    @*<div class="box box-primary">*@
                    <div class="row clearfix">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="input-group">
                                <span class="input-group-addon">
                                    <i class="material-icons">person</i>
                                </span>
                                <div class="form-line">
                                    <select id="idcity" class="form-control" placeholder="City">
                                        <option>-- Select User Type --</option>
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                    </select>
                                    @*<input type="text" class="form-control date" placeholder="User Type">*@
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="input-group">
                                <span class="input-group-addon">
                                    <i class="material-icons">person</i>
                                </span>
                                <div class="form-line">
                                    <input type="text" class="form-control date" placeholder="First Name">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="input-group">
                                <span class="input-group-addon">
                                    <i class="material-icons">person</i>
                                </span>
                                <div class="form-line">
                                    <input type="text" class="form-control date" placeholder="Last Name">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row clearfix">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="input-group">
                                <span class="input-group-addon">
                                    <i class="material-icons">email</i>
                                </span>
                                <div class="form-line">
                                    <input type="text" class="form-control email" placeholder="Ex: user@example.com">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="input-group">
                                <span class="input-group-addon">
                                    <i class="material-icons">phone_iphone</i>
                                </span>
                                <div class="form-line">
                                    <input type="text" class="form-control mobile-phone-number" placeholder="Ex: +91:000000000">
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="text" class="form-control" placeholder="Address">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row clearfix">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="form-group">
                                <div class="form-line">
                                    <select id="idcity" class="form-control" placeholder="City">
                                        <option>-- Select City --</option>
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="form-group">
                                <div class="form-line">
                                    <select id="idstate" class="form-control" placeholder="State">
                                        <option>-- Select State --</option>
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="form-group">
                                <div class="form-line">
                                    <select id="idcountry" class="form-control" placeholder="Country">
                                        <option>-- Select Country --</option>
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row clearfix">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                            <div class="form-group">
                                <div class="form-line">
                                    <input type="password" class="form-control" placeholder="Password">
                                </div>
                            </div>
                        </div>
                    </div>
                    @*  <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" placeholder="Category">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="file" class="form-control" placeholder="Quantity">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row clearfix">
                            <div class="form-group">
                                <div id="box" style="width:800px;height:150px;border:ridge">
                                    <textarea name="description" cols="15" rows="6" placeholder="Product Description" class="form-control" required></textarea>
                                </div>
                            </div>
                        </div>*@
                    <button class="btn btn-primary waves-effect" style="margin-left:280px" type="submit">Save</button>
                </form>
            </div>
            @*</section>*@

        </div>
    </div>
    @*</div>*@

    @*</div>*@
</body>
</html>
